{
  "name": "Producto",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "descripcion": {
      "type": "string",
      "required": true
    },
    "referencia": {
      "type": "string",
      "required": true
    },
    "precioVenta": {
      "type": "number",
      "required": true
    },
    "beneficio": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "pedidos": {
      "type": "hasMany",
      "model": "Pedido",
      "foreignKey": "productoId",
      "through": "DetallesPedido"
    },
    "proveedor": {
      "type": "belongsTo",
      "model": "Proveedor",
      "foreignKey": ""
    },
    "tipoProducto": {
      "type": "belongsTo",
      "model": "TipoProducto",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
