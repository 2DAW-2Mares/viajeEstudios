{
  "name": "Centro",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "codigocentro": {
      "type": "number",
      "required": true
    },
    "nombre": {
      "type": "string",
      "required": true,
      "length": 80
    },
    "localidad": {
      "type": "string",
      "length": 50
    },
    "verificado": {
      "type": "boolean"
    }
  },
  "validations": [],
  "relations": {
    "objetivos": {
      "type": "hasMany",
      "model": "Objetivo",
      "foreignKey": "centroId"
    },
    "usuarios": {
      "type": "hasMany",
      "model": "Usuario",
      "foreignKey": "centroId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "administrador",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
